{
  "name": "ruslandem/evemapper",
  "type": "project",
  "description": "An online tool to help you with some aspects of playing the Eve Online game.",
  "keywords": [
    "eveonline"
  ],
  "authors": [
    {
      "name": "Ruslan Demchenko",
      "email": "ruslan.demchenko@gmail.com",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^8.0",
    "doctrine/dbal": "^3.5",
    "fisharebest/algorithm": "^1.6",
    "fruitcake/laravel-cors": "^3.0",
    "guzzlehttp/guzzle": "^7.0.1",
    "laravel/framework": "^9.0",
    "laravel/sanctum": "^3.0",
    "laravel/socialite": "^5.5",
    "laravel/tinker": "^2.5",
    "socialiteproviders/eveonline": "^4.2",
    "timehunter/laravel-google-recaptcha-v3": "^2.5",
    "tkhamez/swagger-eve-php": "^8.0"
  },
  "require-dev": {
    "barryvdh/laravel-debugbar": "^3.7",
    "barryvdh/laravel-ide-helper": "^2.12",
    "spatie/laravel-ignition": "^1.0",
    "fakerphp/faker": "^1.20",
    "laravel/sail": "^1.0.1",
    "mockery/mockery": "^1.4.4",
    "nunomaduro/collision": "^6.1",
    "phpunit/phpunit": "^9.5.10",
    "spatie/laravel-log-dumper": "^1.4",
    "pusher/pusher-php-server": "^7.0"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/",
      "Database\\Factories\\": "database/factories/",
      "Database\\Seeders\\": "database/seeders/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "scripts": {
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover --ansi"
    ],
    "post-update-cmd": [
      "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
    ],
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php artisan key:generate --ansi"
    ]
  },
  "extra": {
    "laravel": {
      "dont-discover": []
    }
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
